<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onKeyPressed="#keyPressed" onKeyReleased="#keyReleased" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GameController">
   <children>
      <ImageView fx:id="gameBackground" fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/wallpaper.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="playerImage" fitHeight="69.0" fitWidth="66.0" layoutX="611.0" layoutY="323.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/skin2.jpg" />
         </image>
      </ImageView>
      <Label fx:id="label" layoutX="400.0" layoutY="120.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="gameButton" layoutX="195.0" layoutY="147.0" mnemonicParsing="false" opacity="0.0" text="Button" />
   </children>
</AnchorPane>
